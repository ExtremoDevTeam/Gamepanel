		"@types/react": "^18.0.27",
		"@types/react-dom": "^18.0.10",
		"acorn-jsx": "^5.3.2",
		"autoprefixer": "^10.4.13",
		"buffer": "^5.7.1",
		"parcel": "latest",
		"postcss": "^8.4.21",
		"process": "^0.11.10",
		"tailwindcss": "^3.2.6",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5"
	}
