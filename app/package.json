	"targets": {
		"meetings": {
			"source": "src/meetings/index.ts",
			"includeNodeModules": true,
			"context": "browser",
			"outputFormat": "global"
		},
		"search": {
			"source": "src/search/index.ts",
			"includeNodeModules": true,
			"context": "browser",
			"outputFormat": "global"
		},
		"viewer": {
			"source": "src/viewer/index.ts",
			"includeNodeModules": true,
			"context": "browser",
			"outputFormat": "global"
		},
		"profile": {
			"source": "src/profile/index.ts",
			"includeNodeModules": true,
			"context": "browser",
			"outputFormat": "global"
		},
		"dashboard": {
			"source": "src/dashboard/index.ts",
			"includeNodeModules": true,
			"context": "browser",
			"outputFormat": "global"
